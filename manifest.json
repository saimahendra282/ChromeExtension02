{
  "manifest_version": 3,
  "name": "Custom CSS for ERP",
  "version": "1.1",
  "description": "I am bored of the red color of the erp website, so i updated the css ",
  "author": "Bejawada Sai Mahendra",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://newerp.kluniversity.in/*"
  ],
"content_scripts": [
    {
      "matches": [
        "https://newerp.kluniversity.in/",
        "https://newerp.kluniversity.in/index.php"
      ],
      "js": ["content.js"]
    },
    {
      "matches": ["https://newerp.kluniversity.in/*"],
      "exclude_matches": [
        "https://newerp.kluniversity.in/",
        "https://newerp.kluniversity.in/index.php"
      ],
      "js": ["other.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["back.png", "runner.gif"],
      "matches": ["https://newerp.kluniversity.in/*"]
    }
  ],
  "icons": {
    "16": "l16.png",
    "48": "l48.png",
    "128": "l128.png"
  }
}